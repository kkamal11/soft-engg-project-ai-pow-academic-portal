{
    "version": 2,
    "functions": {
        "api/main.py": {
            "runtime": "python@3.9",
            "memory": 1024,
            "maxDuration": 30,
            "includeFiles": "app/**,main.py,requirements.txt",
            "excludeFiles": "**/tests/**,**/__pycache__/**,*.md,initialize_vector_store.py,debug_vector_store.py,generate_mock_data.py"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "MAX_WORKERS": "1",
        "VERCEL": "1"
    }
} 